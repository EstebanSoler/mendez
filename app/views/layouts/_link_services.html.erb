<% if TypeOfService.all.any? %>
  <% TypeOfService.last(3).each_with_index do |tos,i| %>
    <% if i == 0 %>
        <li class="first">
          <%= link_to tos.type_name, services_types_path(tos)%>
        </li>
    <% else %>
        <li>
          <%= link_to tos.type_name, services_types_path(tos)%>
        </li>
    <% end %>
  <% end %>
<% end %>
