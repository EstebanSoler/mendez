<div id="sub_menu">
  <div class="row">
    <div class="large-12 columns">
      <dl class="sub-nav">
            <dd><%= link_to  "Volver", :back, :class=>"last" %></dd>
        <% current_service.type_of_service.services.each_with_index do |servicio,i| %>
            <% if i == (current_service.type_of_service.services.count - 1) %>
              <dd class="last"><%= link_to servicio.name, 
              service_path(servicio)%></dd>
            <% else %>
              <dd><%= link_to servicio.name, 
              service_path(servicio)%></dd>
            <% end %>
        <% end %>
      </dl>
    </div>
  </div>
</div>
